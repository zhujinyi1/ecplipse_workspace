package 与或非;

public class 逻辑运算 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		int a=2;
		int b=5;
		//声明布尔型变量（boolean）用于保存&&后的逻辑值
		boolean result1=((a>b)&&(a!=b));
		//声明布尔型变量（boolean）用于保存||后的逻辑值
		boolean result2=((a>b)||(a!=b));
		System.out.println(result1);
		System.out.println(result2);
	}

}
